for i in range(0,len(zetaList)):
    l_back = load(
                e=eqFileName[i],
                c="ZilliCubic11",
                ICP=[2,9],
                NTST=50,
                DS=-0.05,DSMAX=0.05, 
                EPSL=1e-6,EPSU=1e-6,EPSS=1e-3,
                UZSTOP={2:[0.01,7.01]} ) 
    try:
        back = back + run(l_back)
    except:
        back = run(l_back)
#|___1B_BBB. METHOD B
